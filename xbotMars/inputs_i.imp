IMPLEMENTATION
    inputs_i
REFINES
    inputs
SEES
    g_types,
    io_constants,
    lchip_configuration,
    lchip_interface

    // pragma SAFETY_VARS
CONCRETE_VARIABLES
    Obstacle
INVARIANT
    Obstacle : uint8_t
INITIALISATION
    Obstacle := IO_OFF
OPERATIONS
    read_inputs =
    BEGIN
        Obstacle <-- read_global_input(0)
    END;

    po <-- get_Obstacle =
	BEGIN
		po := Obstacle
	END
END
